<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <title>Tool Registration form </title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />

        <link rel="stylesheet" type="text/css" href="Design/bootstrap/css/bootstrap.min.css" />
        <link rel="stylesheet" type="text/css" href="Design/font-awesome/css/font-awesome.min.css" />

        <script type="text/javascript" src="Design/js/jquery-1.10.2.min.js"></script>
        <script type="text/javascript" src="Design/bootstrap/js/bootstrap.min.js"></script>

    </head>
    <body>

        <div class="container">

            <div class="page-header">
                <h1>Capability Enabled <small></small></h1>
            </div>

            <!-- Registration form - START -->
            <div class="container">
                <div class="row">
                    <form role="form" action="FinalUrls" method="POST">
                        <div class="col-lg-6">
                            <div class="well well-sm"><strong><span class="glyphicon glyphicon-asterisk"></span>Required Field</strong></div>
                            <div class="form-group">

                                <div class="container">
                                    <h2>Parameter</h2>
                                    <div class="panel panel-default">
                                        <div class="panel-body">
                                            <div class="form-group" style="margin-left: 30pt;";">
                                                <label for="InputName">Name</label>
                                                <div class="input-group">
                                                    <input type="text" class="form-control" name="Name" id="InputName" placeholder="" required>
                                                    <span class="input-group-addon"><span class="glyphicon glyphicon-asterisk"></span></span>
                                                </div>
                                            </div>
                                            <div class="form-group" style="margin-left: 30pt;";">
                                                <label for="InputName">Variable</label>
                                                <div class="input-group">
                                                    <input type="text" class="form-control" name="Variable" id="InputName" placeholder="" required>
                                                    <span class="input-group-addon"><span class="glyphicon glyphicon-asterisk"></span></span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <input type="submit" name="submit" id="submit" value="Submit" class="btn btn-info pull-right">
                            </div>

                        </div>
                    </form>

                </div>
                <!-- Registration form - END -->

            </div>
        </div>


    </body>
</html>
