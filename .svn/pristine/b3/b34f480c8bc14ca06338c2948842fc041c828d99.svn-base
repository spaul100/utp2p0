<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LTIToolProxyRegistration</servlet-name>
        <servlet-class>org.utp.lti2.LTIToolProxyRegistration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LTILaunch</servlet-name>
        <servlet-class>org.utp.lti2.LTILaunch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HelloTool</servlet-name>
        <servlet-class>org.utp.HelloTool.HelloTool</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>org.ekshiksha.utp.toolregestration.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Checkpara</servlet-name>
        <servlet-class>org.utp.toolRegistration.Checkpara</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Check</servlet-name>
        <servlet-class>org.utp.toolRegistration.Check</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CapabilityParameterSubmit</servlet-name>
        <servlet-class>org.utp.toolRegistration.CapabilityParameterSubmit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FinalUrls</servlet-name>
        <servlet-class>org.utp.toolRegistration.FinalUrls</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubmitToolProfile</servlet-name>
        <servlet-class>org.utp.toolRegistration.SubmitToolProfile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubmitCapabilityDetails</servlet-name>
        <servlet-class>org.utp.toolRegistration.SubmitCapabilityDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginVerification</servlet-name>
        <servlet-class>org.utp.toolRegistration.LoginVerification</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LTIToolProxyRegistration</servlet-name>
        <url-pattern>/lti/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LTILaunch</servlet-name>
        <url-pattern>/lti/launch/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HelloTool</servlet-name>
        <url-pattern>/ekShiksha/lti/helloTool</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Checkpara</servlet-name>
        <url-pattern>/Checkpara</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Check</servlet-name>
        <url-pattern>/Check</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CapabilityParameterSubmit</servlet-name>
        <url-pattern>/CapabilityParameterSubmit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FinalUrls</servlet-name>
        <url-pattern>/FinalUrls</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubmitToolProfile</servlet-name>
        <url-pattern>/SubmitToolProfile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubmitCapabilityDetails</servlet-name>
        <url-pattern>/SubmitCapabilityDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginVerification</servlet-name>
        <url-pattern>/LoginVerification</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
